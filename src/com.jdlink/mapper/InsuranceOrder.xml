<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jdlink.mapper.InsuranceOrderMapper" >

<resultMap id="InsuranceOrderRM" type="InsuranceOrder">
    <association property="InsuranceOrderItem" column="id" select="getInsuranceOrderItem"></association>
</resultMap>

    <select id="getInsuranceOrderItem" resultType="InsuranceOrderItem">
        select * from data_dictionary_item where order_id=#{id};
    </select>

    <select id="listInsuranceOrder" resultMap="InsuranceOrderRM">
        select * from insurance_order ;
    </select>

</mapper>